<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="gov.nrel.nbc.labelprinting.model.Origins" table="origins" catalog="algae_tracker" >
		
		<id name="originId" column="origin_id" type="long">
			<generator class="native"/>
		</id>

         <property name="name" type="string">
            <column name="name">
                <comment>
                	origin name
                </comment>
            </column>
        </property>

         <property name="description" type="string">
            <column name="description">
                <comment>
                	origin description
                </comment>
            </column>
        </property>
	</class>
</hibernate-mapping>