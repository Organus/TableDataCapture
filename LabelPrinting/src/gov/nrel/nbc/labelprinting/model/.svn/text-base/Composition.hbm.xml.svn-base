<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="gov.nrel.nbc.labelprinting.model.Composition" table="compositions" catalog="tracker" >
		
		<id name="comp_id" column="comp_id" type="long">
			<generator class="native"/>
		</id>


         <property name="name" type="string">
            <column name="name">
                <comment>
                	composition name
                </comment>
            </column>
        </property>
	
	</class>
</hibernate-mapping>
