package gov.nrel.nbc.labelprinting.model;

/**
 * This is a data transfer object class. A <Feedstocks> represents a
 * feedstock for a sample.
 * 
 * @author jalbersh
 */
public class Feedstocks {
	
	/**
	 * Generated unique identifier with no business meaning
	 */
	private long id;
	
	/**
	 * Display name.
	 */
	private String name;
	
	public Feedstocks() {
		name = null;
	}
	
	public Feedstocks(String value) {
		name = value;
	}
	
	public long getId() {
		return id;
	}
	
	public void setId(long id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
}
