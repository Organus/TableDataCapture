package gov.nrel.nbc.labelprinting.model;

import java.io.Serializable;

public class Custodians implements Serializable {
	private static final long serialVersionUID = 7093043845578377691L;

	/**
	 * Generated unique identifier with no business meaning
	 */
	private long id;
	
	/**
	 * Display name.
	 */
	private String name;
	
	/**
	 * Simple constructor
	 */
	public Custodians() {
		name = null;
	}
	
	/**
	 * Complex constructor
	 * 
	 * @param value
	 */
	public Custodians(String value) {
		name = value;
	}
	
	public long getId() {
		return id;
	}
	
	public void setId(long id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
}
