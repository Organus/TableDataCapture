<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="gov.nrel.nbc.tracker.model.Trb" table="trb" catalog="tracker">
		
		<id
			name="id"
			column="trb_data_id">
			<generator class="increment"/>
		</id>
		
		<property
			name="num"
			column="trb_num"
			index="num_page_index"
			unique-key="num_page" not-null="false"/>
    	
		<property
			name="page"
			column="trb_page"
			index="num_page_index"
			unique-key="num_page" not-null="false"/>
    	

    	<many-to-one name="trbFile"
			class="gov.nrel.nbc.tracker.model.CalcSSFile"
            column="calc_ss_file_id"
            not-null="false"/>

		
	</class>
</hibernate-mapping>
