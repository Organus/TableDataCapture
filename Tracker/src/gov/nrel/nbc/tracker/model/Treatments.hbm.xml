<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="gov.nrel.nbc.tracker.model.Treatments" table="treatments" catalog="tracker" >
		
		<id name="id" column="treatments_id" type="long">
			<generator class="native"/>
		</id>


         <property name="name" type="string">
            <column name="name">
                <comment>
                	treatment name
                </comment>
            </column>
        </property>
	
	</class>
</hibernate-mapping>
