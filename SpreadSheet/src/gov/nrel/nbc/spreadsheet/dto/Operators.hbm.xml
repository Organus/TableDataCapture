<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="gov.nrel.nbc.spreadsheet.dto.Operators" table="operators">
		
		<id name="id" column="operator_id">
			<generator class="increment"/>
		</id>
		
		<property
			name="operator"
			column="operator"/>
		
    	<many-to-one name="data_type_id"
			class="gov.nrel.nbc.spreadsheet.dto.DataType"
            column="data_type_id"
            not-null="false"/>
		
	
	</class>
</hibernate-mapping>
