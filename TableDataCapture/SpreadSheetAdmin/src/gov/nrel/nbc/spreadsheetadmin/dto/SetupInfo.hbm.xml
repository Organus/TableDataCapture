<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="gov.nrel.nbc.spreadsheetadmin.dto.SetupInfo" table="setup_info" lazy="false">
		
		<id name="setup_id" 
		    column="setup_id">
			<generator class="increment"/>
		</id>
		
		<property
			name="title"
			column="title"
			not-null="true"/>
		
		<property
			name="displayUrls"
			column="displayUrls"
			not-null="true"/>
		
		<set name="urls" table="setup_info_url_join" cascade="all" lazy="false">
			<key column="setup_id"/>
			<many-to-many column="url_id" class="gov.nrel.nbc.spreadsheetadmin.dto.SetupUrls"/>
		</set>

	</class>
</hibernate-mapping>
