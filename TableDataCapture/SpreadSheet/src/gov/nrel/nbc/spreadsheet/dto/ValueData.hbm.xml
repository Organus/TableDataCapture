<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="gov.nrel.nbc.spreadsheet.dto.ValueData" table="value_data">
		
		<id name="data_id" column="data_id">
		</id>

		<property
			name="data_type"
			column="data_type"/>
		
		<property
			name="lvalue"
			column="lvalue"/>
		
		<property
			name="rvalue"
			column="rvalue"/>
		
		<property
			name="svalue"
			column="svalue"/>
		
		<property
			name="dvalue"
			column="dvalue"/>
		
		<property
			name="bvalue"
			column="bvalue"/>

	</class>
</hibernate-mapping>
